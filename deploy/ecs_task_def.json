{
  "family": "strapi-task-reshma",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::<ACCOUNT_ID>:role/ecsTaskExecutionRole-reshma",
  "containerDefinitions": [
    {
      "name": "strapi-reshma",
      "image": "<IMAGE_URI>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 1337,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
